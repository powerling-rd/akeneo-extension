{
    "name": "powerling/akeneo-extension",
    "type": "symfony-bundle",
    "description": "Powerling extension for Akeneo product data",
    "license": "OSL-3.0",
    "authors": [
        {"name": "Powerling", "homepage": "https://powerling.com"}
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "require": {
	"php": "7.3.*",
        "ext-dom": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "behat/behat": "3.*",
        "friendsofphp/php-cs-fixer": "2.*",
        "phpspec/phpspec": "6.*",
        "akeneo/pim-community-dev": "4.0.*@stable"
    },
    "autoload": {
        "psr-4": {
            "Pim\\Bundle\\PowerlingBundle\\": "src/"
        }
    }
}
